# ============================================
# YieldProp - Environment Configuration
# ============================================

# ----- Ethereum / RPC -----
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Chainlink CRE (for cre-workflow simulation)
# Same as PRIVATE_KEY - 64-char hex, NO 0x prefix
CRE_ETH_PRIVATE_KEY=your_64_character_private_key_here

# ----- Tenderly Virtual TestNet -----
# Option 1: Create via API — set these 3 vars, then run: npm run create:tenderly-vnet
TENDERLY_ACCESS_KEY=your_tenderly_access_token
TENDERLY_ACCOUNT_SLUG=your_account_slug
TENDERLY_PROJECT_SLUG=your_project_slug
# Option 2: Create via dashboard at https://dashboard.tenderly.co → Virtual TestNets → Create
TENDERLY_VIRTUAL_TESTNET_RPC=https://virtual.sepolia.rpc.tenderly.co/your-vnet-id
# Chain ID of your Virtual TestNet (11155111 for Sepolia fork)
TENDERLY_CHAIN_ID=11155111

# ----- Contract Verification -----
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# ----- AI / External APIs -----
OPENAI_API_KEY=your_openai_api_key_here

# Market Data API (served by the dashboard — no paid key required)
# For local dev: http://localhost:3000/api/market-data
# For production: https://your-deployed-dashboard.vercel.app/api/market-data
MARKET_DATA_API_URL=http://localhost:3000/api/market-data

# ----- Property Configuration -----
PROPERTY_ADDRESS=123 Main St, San Francisco, CA
PROPERTY_TYPE=Single Family
PROPERTY_VALUATION=5000
# Optional: override current rent (USD/month). If unset, orchestrator fetches from PriceManager contract
PROPERTY_CURRENT_RENT=

# ----- Contract Addresses (populated after deployment) -----
PROPERTY_TOKEN_ADDRESS=
PRICE_MANAGER_ADDRESS=
YIELD_DISTRIBUTOR_ADDRESS=
PROPERTY_SALE_ADDRESS=
MOCK_USDC_ADDRESS=

# ----- Role Addresses -----
PROPERTY_MANAGER_ADDRESS=
